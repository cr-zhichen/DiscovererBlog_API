{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "MySQL": "server=[数据库IP];user id=[用户名];password=[密码];port=3306;database=[数据库名]"
  },
  "UseUrls": "http://*:8990",
  "Jwt": {
    "SecretKey": "[Key 随便填]",
    "Issuer": "[Issuer 随便填]",
    "Audience": "[Audience 随便填]"
  },
  "SMTP": {
    "SmtpService": "smtp.office365.com",
    "Port": 587,
    "SendEmail": "[发送邮箱]",
    "SendPwd": "[发送密码]"
  }
}
